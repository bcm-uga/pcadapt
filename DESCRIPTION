Package: pcadapt
Type: Package
Title: Fast Principal Component Analysis for Outlier Detection
Version: 3.0.4
Date: 2016-12-20
Authors@R: c(person("Keurcien","Luu", role = c("aut","cre"), email = "keurcien.luu@imag.fr"), 
          person("Michael","Blum", role = c("aut"), email = "michael.blum@imag.fr"),
